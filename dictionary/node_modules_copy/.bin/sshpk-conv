#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/SimpleDictonaryApp/Task-MERN/dictionary/node_modules/sshpk/bin/sshpk-conv/node_modules:/home/workspace/SimpleDictonaryApp/Task-MERN/dictionary/node_modules/sshpk/bin/node_modules:/home/workspace/SimpleDictonaryApp/Task-MERN/dictionary/node_modules/sshpk/node_modules:/home/workspace/SimpleDictonaryApp/Task-MERN/dictionary/node_modules:/home/workspace/SimpleDictonaryApp/Task-MERN/node_modules:/home/workspace/SimpleDictonaryApp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-conv" "$@"
else 
  exec node  "$basedir/../sshpk/bin/sshpk-conv" "$@"
fi
